#include <iostream>
using namespace std;
int main() {
  int x1,x2,x3,x4,y1,y2,y3,y4; char emel;
  cout<<"\n\n\t\tBu diskret fenninden sade bir kalkulyatordur.";
  cout<<"\n\nProqramin muellifi Sabah qrupunun telebesi Cabbarli Fexriyyedir.30.09.2014";
  cout<<"\n\n   \t\t Burada evvelce hansi emeli yerine yetireceyinizi qeyd edin.";
  cout<<"\n\nv ile vebb, i ile implikasiya, f ile x in, t ile y in inkari qeyd olunmusdur.";
  while(1){
cin>>emel;
cin>>x1>>y1;
cin>>x2>>y2;
cin>>x3>>y3;
cin>>x4>>y4;

cout<<"f(x,y)=";


if(emel=='~')
{
if(x1==y1)cout<<"1";else cout<<"0";
if(x2==y2)cout<<"1";else cout<<"0";
if(x3==y3)cout<<"1";else cout<<"0";
if(x4==y4)cout<<"1";else cout<<"0";
}
if(emel=='+')
{
    if(x1!=y1)cout<<"1";else cout<<"0";
    if(x2!=y2)cout<<"1";else cout<<"0";
    if(x3!=y3)cout<<"1";else cout<<"0";
    if(x4!=y4)cout<<"1";else cout<<"0";
}
if(emel=='^')
{
    if(x1<=y1)cout<<x1;else cout<<y1;
    if(x2<=y2)cout<<x2;else cout<<y2;
    if(x3<=y3)cout<<x3;else cout<<y3;
    if(x4<=y4)cout<<x4;else cout<<y4;
}
if(emel=='0')
{
    cout<<"0000";
}
if(emel=='1')
{
    cout<<"1111";
}
if(emel=='d')
{
    if(x1>=y1)cout<<x1;else cout<<y1;
    if(x2>=y2)cout<<x2;else cout<<y2;
    if(x3>=y3)cout<<x3;else cout<<y3;
    if(x4>=y4)cout<<x4;else cout<<y4;
}
if(emel=='v')
{
    if(x1==0&&y1==0)cout<<"1";else cout<<"0";
    if(x2==0&&y2==0)cout<<"1";else cout<<"0";
    if(x3==0&&y3==0)cout<<"1";else cout<<"0";
    if(x4==0&&y4==0)cout<<"1";else cout<<"0";
}if(emel=='i')
{
    if(x1==1&&y1==0)cout<<"0";else cout<<"1";
    if(x2==1&&y2==0)cout<<"0";else cout<<"1";
    if(x3==1&&y3==0)cout<<"0";else cout<<"1";
    if(x4==1&&y4==0)cout<<"0";else cout<<"1";
}
if(emel=='x')
{
    if(x1==0)cout<<"0";else cout<<"1";
    if(x2==0)cout<<"0";else cout<<"1";
    if(x3==0)cout<<"0";else cout<<"1";
    if(x4==0)cout<<"0";else cout<<"1";
}
if(emel=='y')
{
    if(y1==0)cout<<"0";else cout<<"1";
    if(y2==0)cout<<"0";else cout<<"1";
    if(y3==0)cout<<"0";else cout<<"1";
    if(y4==0)cout<<"0";else cout<<"1";
}
if(emel=='f')
{
    if(x1==0)cout<<"1";else cout<<"0";
    if(x2==0)cout<<"1";else cout<<"0";
    if(x3==0)cout<<"1";else cout<<"0";
    if(x4==0)cout<<"1";else cout<<"0";
}
if(emel=='t')
{
    if(y1==0)cout<<"1";else cout<<"0";
    if(y2==0)cout<<"1";else cout<<"0";
    if(y3==0)cout<<"1";else cout<<"0";
    if(y4==0)cout<<"1";else cout<<"0";
}
if(emel=='|')
{
    if(x1==1&&y1==1)cout<<"0";else cout<<"1";
    if(x2==1&&y2==1)cout<<"0";else cout<<"1";
    if(x3==1&&y3==1)cout<<"0";else cout<<"1";
    if(x4==1&&y4==1)cout<<"0";else cout<<"1";
}
  }
return 0;}
